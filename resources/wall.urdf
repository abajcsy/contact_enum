    <inertial>
      <mass value="1000" />
      <origin xyz="3 0 0" rpy="0 0 0" />
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0" />
    </inertial>
    <visual>
      <origin xyz="3 0 0" rpy="0 0 0" />
      <geometry>
        <box size="2 1000 1000" />
      </geometry>
      <material>
        <color rgba="0.93 .74 .4 1" />
      </material>
    </visual>
    <collision group="wall">
      <origin xyz="3 0 0" rpy="0 0 0" />
      <geometry>
        <box size="2 1000 1000" />
      </geometry>
    </collision>
  </link>
</robot>
